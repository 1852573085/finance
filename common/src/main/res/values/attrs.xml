<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="leftText" format="string"></attr>
        <attr name="leftIcon" format="reference"></attr>

        <attr name="title" format="string"></attr>

        <attr name="rightText" format="string"></attr>
        <attr name="rightIcon" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="FinalceProView">
        <attr name="textColor" format="color"></attr>
        <attr name="textSize" format="dimension"></attr>

        <attr name="stork_width" format="dimension"></attr>
        <attr name="stork_color" format="color"></attr>
        <attr name="sweepangle" format="float"></attr>
        <attr name="txtcontent" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="BubbleView">
        <attr name="bubbleText" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="ProgressView">
        <attr name="backColor" format="color"></attr>
        <attr name="proColor" format="color"></attr>
        <attr name="proTextColor" format="color"></attr>
        <attr name="ProProgress" format="float"></attr>
    </declare-styleable>
</resources>